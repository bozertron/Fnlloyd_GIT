{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Fnlloyd",
  "version": "0.1.0",
  "identifier": "com.fnlloyd.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "!Fnlloyd - Defend The Earth",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "center": true,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}
